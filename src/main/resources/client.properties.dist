# Providers
provider.RepositoryProvider=ghtorrent|github|jgit
provider.PullRequestProvider=ghtorrent|github|jgit
provider.SingleDecorators=cache-read,ghtorrent,github,jgit,cache-write
provider.PairwiseDecorators=cache-read,ghtorrent,github,jgit,cache-write

# Settings
settings.large=1000
settings.pairs.skipLarge=false
settings.pairs.skipDifferentTargets=true

# Cache provider
cache.Directory=/path/to/cache

# GHTorrent provider
ghtorrent.Host=localhost
ghtorrent.Port=3306
ghtorrent.User=mysql-user
ghtorrent.Password=mysql-password
ghtorrent.Database=mysql-database

# GitHub provider
github.Owner=username
github.Repository=repository-name
github.PersonalAccessToken=123456789abcdef

# JGit provider
jgit.Directory=/path/to/local/git/repo
jgit.Clean=true
